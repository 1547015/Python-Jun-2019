class Animal:  
    def speak(self):  
        print("Animal Speaking")  
    def smile(self):
    	print("Animal smiling")    


#The child class Dog inherits the base class Animal  
class Dog(Animal):  
    def bark(self):  
        print("dog barking")    
    def speak(self, x=None):
    	if(x==None):  
    		print("DOG is DUMB")
    	else:
    		print("DOG ",x)    
class Dogchild(Dog):
	def bark1(self):  
		print("dog barking from dog child") 
d = Dog()  
c = Animal()
x = Dogchild()
x.bark()  
x.speak() 
x.speak("barks")
#d.eat()  
